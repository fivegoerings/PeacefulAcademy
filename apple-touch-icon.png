<!-- This is a placeholder for apple-touch-icon.png - in a real implementation, this would be a PNG file -->
<!-- For now, we'll create an SVG that can be converted to PNG format -->
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180">
  <rect width="180" height="180" rx="36" fill="#0b1226"/>
  
  <!-- Gradient definitions -->
  <defs>
    <linearGradient id="bookGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#22c55e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#06b6d4;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="capGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f59e0b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ef4444;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Book -->
  <g transform="translate(90, 110)">
    <!-- Book spine -->
    <rect x="-4" y="-45" width="8" height="90" rx="4" fill="url(#bookGradient)"/>
    
    <!-- Book pages -->
    <rect x="-30" y="-45" width="26" height="90" rx="4" fill="#ffffff" opacity="0.95"/>
    <rect x="4" y="-45" width="26" height="90" rx="4" fill="#ffffff" opacity="0.95"/>
    
    <!-- Book cover -->
    <rect x="-30" y="-45" width="60" height="90" rx="4" fill="url(#bookGradient)" opacity="0.8"/>
    
    <!-- Book title lines -->
    <rect x="-20" y="-35" width="15" height="2" rx="1" fill="#ffffff" opacity="0.8"/>
    <rect x="-20" y="-30" width="25" height="2" rx="1" fill="#ffffff" opacity="0.6"/>
    <rect x="-20" y="-25" width="20" height="2" rx="1" fill="#ffffff" opacity="0.6"/>
    
    <!-- Page lines -->
    <rect x="-25" y="-15" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="-12" width="18" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="-9" width="22" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="-6" width="17" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="-3" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="0" width="16" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="3" width="19" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="6" width="21" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="9" width="18" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="12" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="15" width="17" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="18" width="22" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="21" width="19" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="24" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="27" width="18" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="30" width="21" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="-25" y="33" width="17" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    
    <!-- Right page lines -->
    <rect x="5" y="-15" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="-12" width="18" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="-9" width="22" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="-6" width="17" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="-3" width="20" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="0" width="16" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="3" width="19" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="6" width="21" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
    <rect x="5" y="9" width="18" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
