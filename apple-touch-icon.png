    <rect x="5" y="33" width="17" height="1" rx="0.5" fill="#374151" opacity="0.3"/>
  </g>
  
  <!-- Graduation Cap -->
  <g transform="translate(90, 45)">
    <!-- Cap base -->
    <ellipse cx="0" cy="0" rx="30" ry="8" fill="url(#capGradient)"/>
    
    <!-- Cap top -->
    <path d="M -22 0 Q 0 -15 22 0" stroke="url(#capGradient)" stroke-width="3" fill="none" stroke-linecap="round"/>
    
    <!-- Tassel -->
    <circle cx="0" cy="-13" r="1.5" fill="#fbbf24"/>
    <path d="M 0 -13 L 0 -20" stroke="#fbbf24" stroke-width="1" fill="none"/>
    <path d="M 0 -20 L -3 -25" stroke="#fbbf24" stroke-width="1" fill="none"/>
    <path d="M 0 -20 L 3 -25" stroke="#fbbf24" stroke-width="1" fill="none"/>
